.define SELIN 11        ; Set to proper SELIN

.program dss

; DSS - Sound part with FIFOCLK (FIFOFULL is not handled by PIO!)
; INPUTS: D0-D7
; OUTPUTS: none

.wrap_target
    wait 1 gpio SELIN
    wait 0 gpio SELIN   ; Wait for WE edge (SELIN)
    in pins, 8          ; Read data (D0-D7)
    push block          ; Send data
.wrap